{
  "name": "elquire",
  "version": "1.0.1",
  "description": "Local Node dependencies without the hassle: require by a given name instead of a relative path.",
  "main": "index.js",
  "author": "Sam Gluck <sdgluck@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdgluck/elquire.git"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/_mocha test",
    "lint": "standard ./src/*.js",
    "build": "babel ./src --out-dir ./",
    "check": "npm run -s lint && npm run -s test",
    "prepublish": "npm run check && npm run -s build"
  },
  "bugs": {
    "url": "https://github.com/sdgluck/elquire/issues"
  },
  "keywords": [
    "elquire",
    "local",
    "dependency",
    "dependencies",
    "module",
    "modules",
    "loader",
    "extension",
    "transform",
    "path",
    "require"
  ],
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.2.0",
    "babel-eslint": "^5.0.0-beta4",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.5.2",
    "catch-and-match": "^0.2.10",
    "expect": "^1.13.0",
    "mocha": "^2.3.4",
    "standard": "^6.0.5"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "find-package-json": "^1.0.0"
  }
}
